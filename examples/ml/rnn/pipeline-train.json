{
  "pipeline": {
    "name": "GoT_train"
  },
  "transform": {
    "image": "gcr.io/aerobic-optics-216408/tensor_flow_rnn_got:v1.2.6-gpu",
    "cmd": [ "sh" ],
    "stdin": [
      "cd /code && python ptb_word_lm.py --data_path=/pfs/GoT_scripts/data/data/ --model=small --model_path_prefix=/pfs/out/"
    ]
  },
  "resource_limits": {
    "memory": "1000M",
    "gpu": {
      "type": "nvidia.com/gpu",
      "number": 1
    }
  },
  "input": {
    "pfs": {
      "repo": "GoT_scripts",
      "glob": "/"
    }
  }
}
